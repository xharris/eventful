{
  "name": "eventful",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production yarn run api",
    "rs:start": "react-scripts start",
    "dev:build": "react-scripts build",
    "postbuild": "ts-node scripts/postbuild.ts",
    "test": "playwright test",
    "test:debug": "playwright test --debug --project=firefox",
    "test:server": "cross-env NODE_ENV=test BROWSER=none yarn run dev",
    "eject": "react-scripts eject",
    "api": "nodemon --config nodemon.json api/index.ts",
    "dev": "concurrently -n api,web \"yarn run dev:api\" \"yarn run dev:web\"",
    "dev:api": "cross-env PORT=3000 yarn run api",
    "dev:web": "cross-env PORT=3001 yarn run rs:start"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-popover": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@stitches/react": "^1.2.8",
    "@tanstack/react-query": "^4.0.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "casual": "^1.6.2",
    "connect-mongodb-session": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "firebase": "^9.9.2",
    "firebase-admin": "^11.0.1",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "mongoose": "^6.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-responsive": "^9.0.0-beta.10",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "react-select": "^5.4.0",
    "react-textarea-autosize": "^8.3.4",
    "react-use": "^17.4.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "tinycolor": "^0.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.25.0",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-mongodb-session": "^2.4.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.5",
    "@types/morgan": "^1.9.3",
    "@types/react-textarea-autosize": "^8.0.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0"
  }
}
